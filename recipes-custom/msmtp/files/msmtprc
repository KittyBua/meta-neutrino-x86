# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        /var/log/mail.log
port	       587

# gmail
account        gmail
host           smtp.gmail.com
from           MAIL
user           MAIL
password       PASSWD

# t-online
account        t-online
host           securesmtp.t-online.de
from           MAIL
user           MAIL
password       PASSWD

#web.de
account	       web.de
host 	       smtp.web.de
from	       MAIL
user 	       MAIL
password       PASSWD

#gmx
